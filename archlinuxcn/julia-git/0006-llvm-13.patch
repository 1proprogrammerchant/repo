diff --git a/Makefile b/Makefile
index 3452310148..a6a1587bcd 100644
--- a/Makefile
+++ b/Makefile
@@ -189,7 +189,7 @@ else
 JL_PRIVATE_LIBS-$(USE_SYSTEM_ZLIB) += libz
 endif
 ifeq ($(USE_LLVM_SHLIB),1)
-JL_PRIVATE_LIBS-$(USE_SYSTEM_LLVM) += libLLVM libLLVM-12jl
