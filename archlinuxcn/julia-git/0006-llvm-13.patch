 CLANG_JLL_NAME := Clang
-# equivalent to libLLVM 12.0.1+4
-CLANG_JLL_VER  := 12.0.1+3
+CLANG_JLL_VER  := 13.0.0+0
 
 # DSFMT
 DSFMT_VER := 2.2.4
@@ -45,15 +44,14 @@ LIBUV_VER := 2
 LIBUV_JLL_NAME := LibUV
 
 # LLVM
-LLVM_VER := 12.0.1
-LLVM_ASSERT_JLL_VER := 12.0.1+4
